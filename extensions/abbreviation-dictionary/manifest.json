{
  "manifest_version": 3,
  "name": "Abbreviation Dictionary",
  "version": "1.0",
  "description": "Store abbreviations and their meanings.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "downloads"
  ]
}
